	$(document).ready(function(e) {
//		$('.tooltipped').tooltip({delay: 50});
		$(".clear-search").click(function(){
			$("#search").val('');
		});
		if($(".notif").size()>0){
			if($(".notif").is(":visible")){
				setTimeout(function(){
					$(".notif").slideUp();
				},5000);
			}
		}
		$("#psw-s").click(function(e){
			e.preventDefault();
			$("#rl-p,#psw-h").toggle();
			$(this).hide();
			$("#ps-l").hide();
		});
		$("#psw-h").click(function(e){
			e.preventDefault();
			$("#rl-p,#psw-s").toggle();
			$(this).hide();
			$("#ps-l").show();
		});
		$("#vid-report").click(function(e){
			e.preventDefault();
			var elem=$(this);
			$.post("http://vodmine.com/async/",{o:"loginStatus"}).done(function(d){
				var data=$.parseJSON(d);
				if(data.loggedIn){
					$.post("http://vodmine.com/async/",{o:"report",v:"LI5zMiHOLwItvw38HjZV",t:"video"}).done(function(d){
						elem.addClass("red-text",300);
						elem.removeClass("secondary-text-color");
						elem.attr("id","vid-reported");
						elem.find("span").text("Reported");
						elem.removeAttr("href");
						elem.unbind("click");	
					}).fail(function(e){
						alert("Error while reporting the page, please try again later.\nError Code 1: "+e.status);		
					});	
				}else{
					$("body").append('<div id="modal" class="modal"> <div class="modal-content"> <h4><i class="mdi-alert-warning left" style="margin-right:5px;"></i>Alert</h4> <p>Please login to your account.</p> </div> <div class="modal-footer"><a href="#" class="waves-effect btn default-primary-color modal-action modal-close"><i class="mdi-navigation-close left" style="margin-right:0px;"></i>Close</a> <a href="http://vodmine.com/login/?l=/video/LI5zMiHOLwItvw38HjZV" class="waves-effect btn default-primary-color modal-action" style="margin-right:10px;"><i class="mdi-communication-vpn-key left" style="margin-right:5px;"></i>Login</a></div> </div>');
					$('#modal').openModal({
  						  dismissible: true, // Modal can be dismissed by clicking outside of the modal
						  opacity: .5, // Opacity of modal background
						  in_duration: 300, // Transition in duration
						  out_duration: 200, // Transition out duration
						  complete: function() { $("body").find("#modal").remove(); } 	
					});
				}
			}).fail(function(e){
				alert("Error while reporting the page, please try again later.\nError Code: "+e.status);
			});
		});
		$("#sm-screen").click(function(e){
			e.preventDefault();
			$("#mc").addClass("l8");
			$("#mc").removeClass("l12");
		});
		$("#big-screen").click(function(e){
			e.preventDefault();
			$("#mc").addClass("l12");
			$("#mc").removeClass("l8");
		});
		$('.video').matchHeight();	
		$("#seesc").click(function(e){
				e.preventDefault();
				if($("li.sl").is(":visible")){$("li.sl").slideUp();$(this).find("a").html('<i class="mdi-navigation-arrow-drop-down right" style="margin-left:5px;"></i>Show More');}else{$("li.sl").slideDown();$(this).find("a").html('<i class="mdi-navigation-arrow-drop-up right" style="margin-left:5px;"></i>Show less');}
		});
		$('.datepicker').pickadate({
			formatSubmit: 'yyyy-mm-dd',
			hiddenName: true,
			closeOnSelect: false	
		});
		$(".dropdown-button").dropdown({ hover: false });
        $(".button-collapse").sideNav();
		$("#share-vid").click(function(e){
			e.preventDefault();
			if(!$("#share-container").is(":visible")){
				var li=$(this).parent();
				$(li.parent().children()).each(function(){
					$(this).removeClass("active");	
				});
				$(li).addClass("active");
				$(".cont").hide();
				$("#share-container").show("slide");
			}
		});
		$("#embed-vid").click(function(e){
			e.preventDefault();	
			if(!$("#embed-container").is(":visible")){
				var li=$(this).parent();
				$(li.parent().children()).each(function(){
					$(this).removeClass("active");	
				});
				$(li).addClass("active");
				$(".cont").hide();
				$("#embed-container").show("slide");
			}
		});
		$("#share-link").click(function(e){
			e.preventDefault();
			$(".share-wrap").toggle("slide");
		});
		$('ul.tabs').tabs();
		$('select').material_select();
    });
</script>
